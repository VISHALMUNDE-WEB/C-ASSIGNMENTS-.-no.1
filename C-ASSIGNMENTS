	int pass;
	
	printf("enter username : ");
	gets(name);
	
	fflush(stdin);
	
	printf("enter the password : ");
	scanf("%d",&pass);
	
	int val = strcmp(username,name);
	
	if(val==0)
	{
		if(pass==1234)
		{
			printf("welcome ajay ");
		}
		else
		{
			printf("invalid password !! ");
		}
		
	}
		
		else 
		{
			printf("you are not a user !! ");
