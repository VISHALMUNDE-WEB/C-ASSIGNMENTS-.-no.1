 26) Program to find Factorial of a number
 
 #include <stdio.h>
int main()
{
    int n,i,f;
    f=i=1;
    printf("Enter a Number to Find Factorial: ");
    scanf("%d",&n);
    while(i<=n)
    {
        f*=i;
        printf("The %d Factorial 0f %d : %d\n",i,n,f);
        i++;
    }
    printf("\n This is end of the code ");
    return 0;
}
========================================================================================================

