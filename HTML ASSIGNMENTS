11 Make a form which accepts name and date of birth of a person and stores it in the database.Use date type to take date of birth.

ANS. PAGE1.HTML

<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Insert title here</title>
</head>
<body>
<form action="ASSI_11">
<fieldset>
<legend>DETAILS</legend>
FULLNAME<input type="text" name="fn">
DOB<input type="date" name="da">
<input type="submit" value="SUBMIT">
</fieldset>
</form>
</body>
</html>

   SERVLET.JSP
   
package ASSIGNMENTS;

import java.beans.Statement;
import java.io.IOException;
import java.io.PrintWriter;
import java.sql.Date;
import java.sql.DriverManager;
import java.text.SimpleDateFormat;

import javax.servlet.ServletException;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import com.sun.corba.se.pept.transport.Connection;

/**
 * Servlet implementation class ASSI_11
 */
public class ASSI_11 extends HttpServlet {
	private static final long serialVersionUID = 1L;
       
    /**
     * @see HttpServlet#HttpServlet()
     */
    public ASSI_11() {
        super();
        // TODO Auto-generated constructor stub
    }

	/**
	 * @see HttpServlet#doGet(HttpServletRequest request, HttpServletResponse response)
	 */
	protected void doGet(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
		response.setContentType("text/html");
		String fnm = request.getParameter("fn");
		
		Date der = null;
		
		String d = request.getParameter("da");
		
		SimpleDateFormat ft1 = new SimpleDateFormat("yyyy-MM-dd");

		PrintWriter pw = response.getWriter();
		
		try {
			
			der = (Date) ft1.parse(d);
			
			String str = ft1.format(der);
			
			pw.print(str);
			
			Class.forName("oracle.jdbc.driver.OracleDriver");
			
			Connection cn = (Connection) DriverManager.getConnection("jdbc:oracle:thin:@localhost:1521:xe","system","jyotimunde");
			
			java.sql.Statement st = ((java.sql.Connection) cn).createStatement();
			
			String query ="insert into ASSI11('"+fnm+"',date'"+d+"')";
			
			((java.sql.Statement) st).executeUpdate(query);
			
			pw.print("<br>INSERTED SUCCSSFULLY INTO THE TABLE........");
		}
	    catch(Exception ea)
	    {
	    	ea.printStackTrace();
	    }
		
	}
	protected void doPost(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
		// TODO Auto-generated method stub
		doGet(request, response);
	}

}
