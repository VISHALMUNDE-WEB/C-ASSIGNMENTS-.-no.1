22) Print the sum of only odd numbers between 1 and 10.

#include<stdio.h>

int main()
{
	int start, end, sum = 0;
	
	printf("enter star and end value\n");
	scanf("%d%d", &start, &end);
	
	printf("\nsum of odd numbers from %d to %d is", start, end);
	
	while(start <= end)
	{
		if(start%2 !=0)
		{
			sum = sum + start;
		}
		start++;
	}
	printf("%d\n", sum);
	return 0;
}
================================================================================================================================

23) Program to check a number is prime or not

#include<stdio.h>
#include<conio.h>

int main()
{
	int i,n,r=0;
	printf("enter any nmuber:");
	scanf("%d" ,&n);
	
	for(i=2;i<=n-1;i++)
	{
		if(n%i==0)
		r=1;
		break;
	}
	if(r==0)
	printf("prime number");
	else
	printf("not prime number");
	getch();
}
===================================================================================================================================

24) Program to find Sum of digits

#include<stdio.h>
#include<conio.h>

void main()
{
	int num, rem, sum =0;
	
	printf("enter a int number\n");
	scanf("%d",&num);
	
	while(num)
	{
		rem = num % 10;
		sum = sum + rem;
		num = num / 10;
	}
	printf("the sum of digits is %d",sum);
	getch();
}
=============================================================================================================================

